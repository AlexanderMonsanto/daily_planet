<html>
<head>
  <title>Articles</title>
</head>
<body>
  <h1>Hipster Articles</h1>
  <div id="articles">
    <% for(var i =0; i<articles.length;i++){ %>
     <a href="articles/<%=i%>"><h3 align="left" id="#title"> <%= articles[i]["title"] %> </h3></a>
     <p align="center" id="#title"> <%= articles[i]["article"] %> </p>
     <% } %>
  </div>
</body>
</html>
